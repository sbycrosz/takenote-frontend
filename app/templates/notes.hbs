<div class='col-sm-3'>
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">
        All notes
      </h3>
    </div>
    <div class="list-group">
      {{#each}}
        {{#link-to 'note' this class='list-group-item'}} 
          {{title}}
          {{content}}
        {{/link-to}}
      {{/each}}
    </div>
  </div>

</div>
<div class='col-sm-9'>
  {{outlet}}
</div>


